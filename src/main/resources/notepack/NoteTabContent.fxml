<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="462.0" prefWidth="692.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="notepack.NoteTabContentController">
